{
    "manufacturer": "Yalraz",
    "keyboard_name": "ykb",
    "maintainer": "Yalraz",
    "development_board": "promicro",
    "build": {
        "lto": true
    },
    "diode_direction": "COL2ROW",
    "encoder": {
        "rotary": [
            {"pin_a": "E6", "pin_b": "D7"}
        ],
        "push": "B4"
    },
    "features": {
        "bootmagic": true,
        "encoder": true,
        "extrakey": true,
        "nkro": true,
        "oled": false,
        "rgblight": false
    },
    "matrix_pins": {
        "cols": ["B6", "B2", "B3"],
        "rows": ["B1", "F7", "F6"]
    },
    "rgblight": {
        "animations": {
            "alternating": true,
            "breathing": true,
            "rainbow_mood": true,
            "rainbow_swirl": true,
            "snake": true,
            "static_gradient": true
        },
        "led_count": 6
    },
    "url": "https://github.com/rarepotato8de/3x3macropad",
    "usb": {
        "device_version": "0.0.1",
        "pid": "0x0001",
        "vid": "0x5353"
    },
    "ws2812": {
        "pin": "E6"
    },
    "community_layouts": ["ortho_3x3"],
    "layouts": {
        "LAYOUT_ortho_3x3": {
            "layout": [

                  //Row 1 out of 3

                {"label": "k00", "matrix": [0, 0], "x": 0, "y": 0},
                {"label": "k01", "matrix": [0, 1], "x": 1, "y": 0},
                {"label": "k02", "matrix": [0, 2], "x": 2, "y": 0},

                  //Row 2 out of 3

                {"label": "k10", "matrix": [1, 0], "x": 0, "y": 1},
                {"label": "k11", "matrix": [1, 1], "x": 1, "y": 1},
                {"label": "k12", "matrix": [1, 2], "x": 2, "y": 1},

                  //Row 3 out of 3

                {"label": "k20", "matrix": [2, 0], "x": 0, "y": 2},
                {"label": "k21", "matrix": [2, 1], "x": 1, "y": 2},
                {"label": "k22", "matrix": [2, 2], "x": 2, "y": 2},

            ]
        }
    }
}
